{
  "name": "react-macos",
  "version": "1.0.0",
  "description": "MacOS in the web!",
  "private": true,
  "author": "Hocko <chengkuan689@163.com>",
  "scripts": {
    "dev": "umi dev",
    "build": "umi build",
    "postinstall": "umi setup",
    "setup": "umi setup",
    "start": "npm run dev",
    "lint:fix": "umi lint --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "normalize.css": "^8.0.1",
    "umi": "^4.0.13"
  },
  "devDependencies": {
    "@types/node": "^18.7.11",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/lint": "^4.0.13",
    "eslint": "^8.22.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "stylelint": "^14.11.0",
    "typescript": "^4.1.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,less}": [
      "umi lint"
    ],
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  }
}